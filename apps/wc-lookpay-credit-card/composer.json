{
  "name": "mobilestock/wc-lookpay-credit-card",
  "license": "proprietary",
  "config": {
    "platform": {
      "php": "7.4"
    },
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "php-http/mock-client": "^1.6"
  },
  "autoload": {
    "psr-4": {
