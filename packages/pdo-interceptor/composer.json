{
  "name": "geangontijo/pdo-interceptor",
  "description": "A PDO Interceptor for Laravel",
  "type": "library",
  "autoload": {
    "psr-4": {
      "MobileStock\\PdoInterceptor\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "ext-pdo": "*",
    "illuminate/support": "^11.29",
    "illuminate/database": "^11.29",
    "illuminate/pipeline": "^11.29"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "laravel-zero/foundation": "^8.81"
  },
  "extra": {
    "laravel": {
      "providers": [
        "MobileStock\\PdoInterceptor\\Laravel\\PdoInterceptorServiceProvider"
      ]
    }
  }
}