{
  "name": "mobilestock/pdo-cast",
  "version": "1.0.0",
  "description": "converte os dados do PDO com base nas colunas das tabelas e regras definidas pelo programador",
  "type": "library",
  "autoload": {
    "psr-4": {
      "MobileStock\\PdoCast\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "minimum-stability": "stable",
  "require": {
    "ext-pdo": "*",
    "illuminate/support": "^8.83|^10.0",
    "illuminate/database": "^8.83|^10.0",
    "symfony/polyfill-php81": "^1.29",
    "illuminate/pipeline": "^8.83|^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "laravel-zero/foundation": "^8.81"
  }
}
