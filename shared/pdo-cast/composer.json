{
  "name": "mobilestock/pdo-cast",
  "description": "converte os dados do PDO com base nas colunas das tabelas e regras definidas pelo programador",
  "type": "library",
  "autoload": {
    "psr-4": {
      "MobileStock\\PdoCast\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "ext-pdo": "*",
    "illuminate/support": "^8.83",
    "illuminate/database": "^8.83",
    "symfony/polyfill-php81": "^1.29",
    "illuminate/pipeline": "^8.83"
  },
  "config": {
    "platform": {
      "php": "7.4"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  }
}
