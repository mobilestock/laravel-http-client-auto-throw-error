root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
[*.sql]
indent_size = 4
trim_trailing_whitespace = false
[*.js, *.ts, *.tsx, *.mdx, *.yml]
indent_size = 2
[*_ide_helper.php]
trim_trailing_whitespace = false
